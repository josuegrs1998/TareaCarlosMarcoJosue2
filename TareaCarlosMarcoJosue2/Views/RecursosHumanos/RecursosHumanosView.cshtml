@model IEnumerable<TareaCarlosMarcoJosue2.Models.RecursosHumanosCLS>

@{
    ViewBag.Title = "RecursosHumanosView";
}

<h2>RecursosHumanosView</h2>

<p>Integrantes: Carlos, Marco, Josué</p>

<table class="table table-striped">
    <tr>
